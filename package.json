{
  "name": "react-mobx-router5",
  "version": "0.0.0-development",
  "description": "Router5 integration with mobX and react",
  "homepage": "https://github.com/LeonardoGentile/react-mobx-router5",
  "repository": {
    "type": "git",
    "url": "https://github.com/LeonardoGentile/react-mobx-router5.git"
  },
  "bugs": {
    "url": "https://github.com/LeonardoGentile/react-mobx-router5/issues"
  },
  "author": "Leonardo Gentile",
  "contributors": [],
  "license": "MIT",
  "keywords": [
    "router",
    "mobx",
    "react",
    "router5",
    "functional",
    "routing",
    "observable",
    "reactive",
    "observer"
  ],
  "files": [
    "dist",
    "src",
    "README.md",
    "CHANGELOG.md",
    "CONTRIBUTING.md",
    "LICENSE.txt"
  ],
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "babel": {
    "comments": false,
    "presets": [
      "react",
      "stage-0",
      [
        "es2015",
        {
          "modules": false
        }
      ]
    ],
    "plugins": [
      "transform-decorators-legacy",
      "transform-runtime"
    ]
  },
  "dependencies": {
    "babel-runtime": "^6.11.6",
    "prop-types": "^15.5.8"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.16.3",
    "chai": "3.5.0",
    "chai-enzyme": "1.0.0-beta.0",
    "cheerio": "1.0.0-rc.2",
    "commitizen": "2.9.6",
    "coveralls": "^2.11.14",
    "cz-conventional-changelog": "2.1.0",
    "del": "^2.2.2",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "1.0.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^10.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.10.3",
    "istanbul": "^1.1.0-alpha.1",
    "jsdom": "^9.12.0",
    "mobx": "^5.0.0",
    "mobx-react": "^5.2.0",
    "mobx-router5": "^4.3.0",
    "mocha": "^3.2.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-test-renderer": "16.0.0",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "router5": "6.1.2",
    "semantic-release": "^15.1.7",
    "semantic-release-conventional-commits": "2.0.0",
    "sinon": "^2.0.0-pre.3",
    "sinon-chai": "^2.9.0",
    "travis-deploy-once": "4.4.1"
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "mobx": "^5.0.0",
    "mobx-react": "^5.2.0",
    "mobx-router5": "^4.3.0",
    "router5": ">= 6.1.0 < 7.0.0"
  },
  "scripts": {
    "commit": "git-cz",
    "lint": "eslint src test tools",
    "test": "mocha --compilers js:babel-register --require ./test/utils/test-setup.js 'test/*.js'",
    "test:watch": "mocha --compilers js:babel-register --require ./test/utils/test-setup.js --reporter min --watch 'test/*.js'",
    "test:cover": "babel-node ./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha --require ./test/utils/test-setup.js 'test/*.js'",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "build": "node tools/build",
    "publish:docs": "easystatic deploy docs --repo LeonardoGentile/react-mobx-router5",
    "start": "easystatic start docs",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "release": {
    "analyzeCommits": {
      "path": "semantic-release-conventional-commits",
      "minorTypes": [
        "feat",
        "minor"
      ],
      "patchTypes": [
        "fix",
        "patch",
        "docs",
        "refactor",
        "style",
        "perf",
        "chore"
      ]
    }
  }
}
